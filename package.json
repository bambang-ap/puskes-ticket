		"@mui/material": "5.8.2",
		"@mui/styles": "5.8.0",
		"@preact/signals-react": "^1.3.7",
		"@tanstack/react-query": "4.20.4",
		"@tanstack/react-query-devtools": "4.20.9",
		"@total-typescript/ts-reset": "0.4.2",
		"@trpc/client": "10.11.1",
		"@trpc/next": "10.11.1",
		"@trpc/react-query": "10.11.1",
		"@trpc/server": "10.11.1",
		"axios": "0.27.2",
		"blob-stream": "0.1.3",
		"buffer-to-data-url": "1.0.0",
		"clsx": "1.1.1",
		"date-fns": "2.28.0",
		"global-methods": "bambang-ap/global-methods#2.2.4",
		"jspdf": "^2.5.1",
		"just-clone": "^6.2.0",
		"moment": "2.29.4",
		"moment-timezone": "^0.5.43",
		"next": "13.3.0",
		"next-auth": "4.22.0",
