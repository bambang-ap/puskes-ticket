{
	"name": "puskes-ticket",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"export": "next export",
		"format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --config ./.prettierrc.js",
		"server": "git restore .; git pull; yarn; yarn build; yarn deploy;",
		"deploy": "pm2 kill; pm2 start \"npm run start\" --watch --name \"Puskes Ticket\"; pm2 save; yarn restart;",
		"build": "next lint --fix; next build",
		"start": "NODE_OPTIONS='--max-http-header-size=24576' next start --port 7534;",
		"dev": "NODE_OPTIONS='--max-http-header-size=24576' next dev --port 1234",
		"next-lint": "next lint",
		"lint": "eslint . --ext=jsx,ts,tsx"
	},
	"dependencies": {
		"@emotion/cache": "11.7.1",
		"@emotion/react": "11.9.0",
